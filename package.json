{
    "name": "@anew/store",
    "version": "2.5.2-beta",
    "description": "Anew State Manager",
    "license": "MIT",
    "homepage": "https://github.com/anewjs/store#readme",
    "repository": "github:anewjs/store",
    "bugs": "https://github.com/anewhs/store/issues",
    "author": "Abdelrahman Salem <abubakir1997@gmail.com> (http://www.pages.drexel.edu/\\~aas372/)",
    "keywords": [
        "anew",
        "store",
        "redux",
        "vuex",
        "mobx",
        "relay",
        "reducer",
        "asynchronous",
        "memoization",
        "state",
        "predictable",
        "functional",
        "immutable",
        "hot",
        "live",
        "replay",
        "flux",
        "elm"
    ],
    "main": "lib/anew-store.js",
    "unpkg": "dist/anew-store.js",
    "module": "es/anew-store.js",
    "types": "types/index.d.ts",
    "files": [
        "dist",
        "lib",
        "es",
        "src",
        "types"
    ],
    "scripts": {
        "clean": "rimraf lib dist es coverage types",
        "format": "prettier --write \"{src,test}/**/*.{js,ts}\" index.d.ts \"**/*.md\"",
        "format:check": "prettier --list-different \"{src,test}/**/*.{js,ts}\" index.d.ts \"**/*.md\"",
        "lint": "eslint --ext js,ts src test",
        "check-types": "tsc --noEmit",
        "pretest": "yarn run build",
        "test": "jest",
        "test:watch": "yarn run test -- --watch",
        "test:cov": "yarn run test -- --coverage",
        "build": "rollup -c",
        "prepare": "yarn run clean && yarn run check-types && yarn run format:check && yarn run lint && yarn run test",
        "examples:lint": "eslint --ext js,ts examples",
        "examples:test": "cross-env CI=true babel-node examples/testAll.js"
    },
    "dependencies": {
        "is-class": "^0.0.9",
        "is-plain-object": "^3.0.0",
        "loose-envify": "^1.4.0",
        "reselect": "^4.0.0",
        "symbol-observable": "^1.2.0",
        "type-guards": "^0.12.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.7",
        "@babel/core": "^7.7.7",
        "@babel/node": "^7.7.7",
        "@babel/plugin-external-helpers": "^7.2.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
        "@babel/plugin-transform-react-jsx": "^7.7.7",
        "@babel/preset-env": "^7.7.7",
        "@babel/preset-flow": "^7.0.0",
        "@babel/preset-typescript": "^7.7.7",
        "@babel/register": "^7.7.7",
        "@types/jest": "^24.0.25",
        "@typescript-eslint/eslint-plugin": "^2.13.0",
        "@typescript-eslint/parser": "^2.13.0",
        "babel-eslint": "^10.0.3",
        "babel-jest": "^24.9.0",
        "cross-env": "^6.0.3",
        "eslint": "^6.8.0",
        "eslint-config-react-app": "^5.1.0",
        "eslint-import-resolver-typescript": "^2.0.0",
        "eslint-plugin-flowtype": "^4.5.2",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.17.0",
        "eslint-plugin-react-hooks": "^2.3.0",
        "glob": "^7.1.4",
        "husky": "^3.1.0",
        "jest": "^24.9.0",
        "prettier": "^1.18.2",
        "rimraf": "^3.0.0",
        "rollup": "^1.27.14",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-replace": "^2.2.0",
        "rollup-plugin-terser": "^5.1.3",
        "rollup-plugin-typescript2": "^0.25.3",
        "rxjs": "^6.5.4",
        "typescript": "^3.7.4"
    },
    "npmName": "@anew/store",
    "npmFileMap": [
        {
            "basePath": "/dist/",
            "files": [
                "*.js"
            ]
        }
    ],
    "browserify": {
        "transform": [
            "loose-envify"
        ]
    },
    "jest": {
        "testRegex": "(/test/.*\\.spec\\.[tj]s)$"
    },
    "sideEffects": false
}
